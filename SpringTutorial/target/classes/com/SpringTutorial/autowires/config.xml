<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
 http://www.springframework.org/schema/beans/spring-beans.xsd
 http://www.springframework.org/schema/context
 http://www.springframework.org/schema/context/spring-context.xsd">

<bean name="shop" class="com.SpringTutorial.autowires.Shop" autowire="byName">
<property name="sid" value="101"></property>
<property name="sName" value="Vikas Kirana"></property>
</bean>

<bean name="products" class="com.SpringTutorial.autowires.Product">
<property name="foods" value="Dosa"></property>
<property name="medicine" value="Seredon"></property>
</bean>

</beans>