<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
 http://www.springframework.org/schema/beans/spring-beans.xsd
 http://www.springframework.org/schema/context
 http://www.springframework.org/schema/context/spring-context.xsd">

	<bean id="student" class="com.SpringTutorial.starter.Student">
		<property name="sid" value="101"></property>
		<property name="name" value="Vishal"></property>
		<property name="books">
			<list>
				<value>Applied-Math</value>
				<value>Applied-Physics</value>
				<value>Applied-Chemistry</value>
				<value>History</value>
			</list>
		</property>
	</bean>

</beans>